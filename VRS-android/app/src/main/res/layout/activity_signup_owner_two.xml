<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    android:weightSum="10">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:gravity="center_horizontal|center_vertical"
        android:text="Đăng ký xe"
        android:textColor="#000"
        android:textSize="24sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:text="Nhập tên xe"
        android:textColor="#000"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/edtVehicleName"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1"
        android:hint="Ví dụ: Toyota innova"
        android:padding="0dp"
        android:textSize="16sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:weightSum="10">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="Loại động cơ"
            android:textColor="#000"
            android:textSize="16sp" />

        <Spinner
            android:id="@+id/spnVehicleEngineType"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:drawSelectorOnTop="true"
            android:entries="@array/spinnerItems2">
        </Spinner>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:weightSum="10">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="Loại truyền tải"
            android:textColor="#000"
            android:textSize="16sp" />

        <Spinner
            android:id="@+id/spnVehicleTranmission"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:drawSelectorOnTop="true"
            android:entries="@array/spinnerItems2">
        </Spinner>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:weightSum="10">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="Số chỗ ngồi"
            android:textColor="#000"
            android:textSize="16sp" />

        <Spinner
            android:id="@+id/spnVehicleSeat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:drawSelectorOnTop="true"
            android:entries="@array/spinnerItems2">
        </Spinner>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="horizontal"
        android:weightSum="10"
        android:layout_gravity="center_vertical"
        android:layout_weight="1">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="5"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="#000"
            android:text="Năm sản xuất"/>
        <EditText
            android:id="@+id/edtVehicleYear"
            android:layout_width="0dp"
            android:layout_weight="5"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_marginRight="10dp"
            />
    </LinearLayout>
    <com.google.android.flexbox.FlexboxLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/signup_ico"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        app:alignItems="center"
        app:flexWrap="wrap"
        app:justifyContent="space_around">

        <Button
            android:id="@+id/btnVehicleBack"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/btn_login"
            android:text="Quay lại"
            android:textColor="#FFF"
            android:textSize="16sp"
            app:layout_flexBasisPercent="@fraction/signup_btn" />

        <Button
            android:id="@+id/btnVehicleNext"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/btn_next"
            android:text="Tiếp tục"
            android:textColor="#FFF"
            android:textSize="16sp"
            app:layout_flexBasisPercent="@fraction/signup_btn" />

    </com.google.android.flexbox.FlexboxLayout>


